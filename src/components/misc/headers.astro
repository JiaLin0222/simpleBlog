---
import HeaderLink from "./header-link.astro";
import { menuItems } from "@data/index";
---

<div class='flex flex-row gap-3 font-medium text-black items-start w-full'>
	{
		menuItems.map((item) => (
			<HeaderLink
				href={item.url}
				class='text-black'>
				{item.label}
			</HeaderLink>
		))
	}
</div>
